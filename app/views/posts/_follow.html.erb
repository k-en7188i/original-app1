<% if current_user != post.user %>  
  <% if post.user.followed_by?(current_user) %>
    <%= link_to image_tag("following.png", size: "20x20"), user_relationships_path(post.user), class: "star-icon", method: :delete %>
  <% else %>
    <%= link_to image_tag("notFollowing.png", size: "20x20"), user_relationships_path(post.user), class: "star-icon", method: :post %>
  <% end %>
<% end %>